{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "ESNext"
    ],
    "noEmit": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules"
  ]
}
