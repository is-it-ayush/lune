{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Native.js",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "declarationMap": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "ESNext"
    ],
    "noEmit": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "typeRoots": [
      "./ts-declarations",
      "./node_modules/@types",
      "../../node_modules/@types",
    ],
  },
  "exclude": [
    "node_modules"
  ]
}
